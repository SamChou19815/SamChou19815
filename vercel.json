{
  "buildCommand": "pnpm build && pnpm cp-dist",
  "cleanUrls": true,
  "framework": null,
  "public": true,
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/:path(.*)",
      "has": [
        { "type": "host", "value": "blog.developersam.com" }
      ],
      "destination": "/blog/:path*"
    },
    {
      "source": "/:path(.*)",
      "has": [
        { "type": "host", "value": "www.samlang.io" },
        { "type": "host", "value": "samlang.io" }
      ],
      "destination": "/samlang/:path*"
    },
    {
      "source": "/:path(.*)",
      "has": [
        { "type": "host", "value": "developersam.com" },
        { "type": "host", "value": "www.developersam.com" }
      ],
      "destination": "/www/:path*"
    }
  ]
}
