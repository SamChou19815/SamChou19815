{
  "buildCommand": "pnpm build && pnpm cp-dist",
  "cleanUrls": true,
  "framework": null,
  "public": true,
  "outputDirectory": "dist",
  "redirects": [
    {
      "source": "/:path(.*)",
      "has": [{ "type": "host", "value": "blog.developersam.com" }],
      "destination": "https://developersam.com/blog/:path*"
    }
  ],
  "rewrites": [
    {
      "source": "/:path(.*)",
      "has": [{ "type": "host", "value": "samlang.io" }],
      "destination": "/samlang/:path*"
    },
    {
      "source": "/:path(.*)",
      "has": [{ "type": "host", "value": "developersam.com" }],
      "destination": "/www/:path*"
    }
  ]
}
