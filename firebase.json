{
  "hosting": [
    {
      "target": "blog",
      "public": "packages/blog/build",
      "ignore": ["**/.*"]
    },
    {
      "target": "www",
      "public": "packages/www/out",
      "ignore": ["**/.*"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "queue",
      "public": "packages/queue/build",
      "ignore": ["**/.*"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "samlang",
      "public": "packages/samlang/build",
      "ignore": ["**/.*"]
    },
    {
      "target": "samlang-demo",
      "public": "packages/samlang-demo/build",
      "ignore": ["**/.*"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "tasks",
      "public": "packages/tasks/build",
      "ignore": ["**/.*"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "ten",
      "public": "packages/ten/build",
      "ignore": ["**/.*"],
      "rewrites": [
        {
          "source": "/api/respond",
          "function": "HandleTenAIMoveRequest"
        },
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
