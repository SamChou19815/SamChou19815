{
  "extends": ["airbnb"],
  "parser": "babel-eslint",
  "plugins": [],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "object-curly-newline": [
      "error", {
        "ObjectExpression": {
          "minProperties": 10,
          "multiline": true,
          "consistent": true
        },
        "ObjectPattern": {
          "minProperties": 10,
          "multiline": true,
          "consistent": true
        },
        "ImportDeclaration": {
          "minProperties": 10,
          "multiline": true,
          "consistent": true
        },
        "ExportDeclaration": {
          "minProperties": 10,
          "multiline": true,
          "consistent": true
        }
      }
    ]
  }
}
