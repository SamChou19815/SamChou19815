{
  "plain": {
    "color": "var(--prism-code-block-plain-color)",
    "backgroundColor": "var(--prism-code-block-background-color)"
  },
  "styles": [
    {
      "types": ["comment"],
      "style": {
        "color": "var(--prism-code-block-comment-color)",
        "fontStyle": "italic"
      }
    },
    {
      "types": ["keyword", "char", "tag", "key"],
      "style": {
        "color": "var(--prism-code-block-keyword-color)"
      }
    },
    {
      "types": ["operator", "variable", "punctuation"],
      "style": {
        "color": "var(--prism-code-block-plain-color)"
      }
    },
    {
      "types": [
        "constant",
        "builtin",
        "class-name",
        "attr-name",
        "selector",
        "changed",
        "deleted",
        "property"
      ],
      "style": {
        "color": "var(--prism-code-block-property-color)"
      }
    },
    {
      "types": ["number"],
      "style": {
        "color": "var(--prism-code-block-number-color)"
      }
    },
    {
      "types": ["string"],
      "style": {
        "color": "var(--prism-code-block-string-color)"
      }
    },
    {
      "types": ["function"],
      "style": {
        "color": "var(--prism-code-block-function-color)"
      }
    }
  ]
}
