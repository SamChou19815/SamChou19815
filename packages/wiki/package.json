{
  "name": "wiki",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "codegen": "ts-node --transpile-only --compiler-options '{\"module\": \"commonjs\"}' private-docs-generator.ts",
    "build:public": "docusaurus build",
    "build:private": "yarn codegen && DEV_SAM=true NODE_NO_WARNINGS=1 docusaurus build --out-dir deployment-template/build",
    "build": "yarn build:public && yarn build:private",
    "swizzle": "docusaurus swizzle",
    "compile": "tsc",
    "deploy:public": "yarn common:firebase deploy --token=$FIREBASE_TOKEN --non-interactive --only hosting:wiki",
    "deploy:private": "cd deployment-template && gcloud app deploy",
    "deploy": "yarn deploy:public && yarn deploy:private"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-alpha.58",
    "@docusaurus/preset-classic": "^2.0.0-alpha.58",
    "@mdx-js/react": "^1.6.4",
    "clsx": "^1.1.1",
    "lib-docusaurus-plugin": "0.1.0",
    "lib-react": "0.1.0",
    "prism-react-renderer": "^1.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.0-alpha.58",
    "@types/node": "14.0.22",
    "@types/react": "^16.9.43",
    "ts-node": "^8.10.2",
    "typescript": "3.9.6"
  },
  "githubRepositoryDependencies": [
    "private-monorepo"
  ],
  "deploymentDependencies": [
    "gcloud"
  ],
  "codegenConfiguration": {
    "sources": [
      "../../../private-monorepo/confidential",
      "private-docs-generator.ts",
      "sidebars.json",
      "package.json"
    ],
    "output": "private-docs/private-sidebars.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
