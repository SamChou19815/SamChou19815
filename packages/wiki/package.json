{
  "name": "wiki",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "compile": "tsc",
    "deploy": "firebase deploy --token=$FIREBASE_TOKEN --non-interactive --only hosting:wiki"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-alpha.61",
    "@docusaurus/preset-classic": "^2.0.0-alpha.61",
    "@mdx-js/react": "^1.6.4",
    "clsx": "^1.1.1",
    "firebase": "^7.18.0",
    "lib-docusaurus-plugin": "0.1.0",
    "lib-react": "0.1.0",
    "prism-react-renderer": "^1.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "remarkable": "^2.0.1",
    "rxjs": "^6.6.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.0-alpha.61",
    "@types/node": "^14.0.23",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/remarkable": "1.7.4",
    "typescript": "^3.9.7"
  },
  "githubRepositoryDependencies": [
    "private-monorepo"
  ],
  "deploymentDependencies": [
    "firebase-tools"
  ],
  "codegenConfiguration": {
    "sources": [
      "../../../private-monorepo/confidential",
      "private-docs-generator.ts",
      "sidebars.json",
      "package.json"
    ],
    "output": "private-docs/private-sidebars.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
